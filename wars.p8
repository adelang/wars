pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function _init()
	cursor = {}
	cursor.x = 0  
	cursor.y = 0
	cursor.sprite = 0
	cursor.confirm = false

	block_size = 8
	timer = 0
	frame = 0
end

function _update60()
	--frame = flr(timer / 20)
	--timer = (timer + 1) % 60
	handle_input()
	if cursor.confirm then 
		timer = (timer + 1) % 60 
		frame = flr(timer / 20)

		cursor.sprite = frame

		if frame > 2 then
			cursor.sprite = 0
			cursor.confirm = false
		end
	end
end

function _draw()
	cls()
	draw_background()
	draw_cursor()
	print(timer)
	print(frame)
end

function handle_input()
	if (btnp(0)) then cursor.x -= block_size end
	if (btnp(1)) then cursor.x += block_size end
	if (btnp(2)) then cursor.y -= block_size end
	if (btnp(3)) then cursor.y += block_size end
	if (btnp(4)) then confirm_unit() end
end

function confirm_unit()
	cursor.confirm = true
end

function draw_cursor()
	spr(cursor.sprite, cursor.x, cursor.y)
end

function draw_background()
	map(0, 0, 0, 0)
end
